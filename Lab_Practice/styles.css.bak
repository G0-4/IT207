body
{
	font-family: sans-serif;
	font-size: 20px;
	font-weight: normal;
}
#outer-box
{
	display: flex;
	background-color: lightSalmon;
	border: 3px solid black;
}
#side-menu
{
	background-color: LightBlue;
	width: 20%;
	border: 1px solid black;
	height: 1000px;
}
#content
{
	width: 100%;
	border: 1px solid black;
	position: relative;
}
#header
{
  border: 1px solid black;
  display: flex;
  margin: 5px;
}
#school
{
  text-align: center;
  background-color: PaleGoldenRod;
  width: 50%;
  border: 1px solid black;
  padding: 7px;
}
#me
{
  text-align: center;
  background-color: PaleGoldenRod;
  width: 50%;
  border: 1px solid black;
  padding: 7px;
  display: flex;
}
#photo
{
	border: 3px solid;
	width: 50%;
	float: left;
	background-color: lightBlue;
	border-color: RoyalBlue;
}
#summary
{
	border: 3px solid;
	width: 50%;
	float: left;
	background-color: lightBlue;
	border: 3px solid;
	border-color: RoyalBlue;
}
#details
{
	border: 3px solid;
	width: 49%;
	height: 100%;
	float: right;
	text-align: right;
	background-color: lightBlue;
	border-color: RoyalBlue;
	position: absolute;
	top: 137px;
	right: 0px;
}
#footer
{
	border: 3px solid;
	position: absolute;
	background-color: lightGreen;
	border-color: RoyalBlue;
	display: flex;
	text-align: center;
	bottom: 0;
	width: 100%;
}
